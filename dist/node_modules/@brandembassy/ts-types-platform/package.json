{
  "name": "@brandembassy/ts-types-platform",
  "version": "2.136.0",
  "description": "Platform specific typescript types",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BrandEmbassy/ts-types-platform.git"
  },
  "types": "dist",
  "author": "Brand Embassy",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/BrandEmbassy/ts-types-platform/issues"
  },
  "homepage": "https://github.com/BrandEmbassy/ts-types-platform#readme",
  "devDependencies": {
    "@types/jest": "^25.1.4",
    "@types/node": "^13.9.3",
    "husky": "^3.0.8",
    "jest": "^25.1.0",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1",
    "ts-jest": "^25.2.1",
    "typescript": "^3.6.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "scripts": {
    "clear": "rm -rf ./dist/*",
    "build": "./node_modules/.bin/tsc",
    "test": "./node_modules/.bin/jest --verbose"
  },
  "dependencies": {
    "@types/yup": "^0.26.33",
    "yup": "^0.28.3"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testPathIgnorePatterns": [
      "dist"
    ],
    "testRegex": "\\.(spec|test)\\.(ts|js)$",
    "moduleDirectories": [
      "node_modules"
    ]
  }
}
